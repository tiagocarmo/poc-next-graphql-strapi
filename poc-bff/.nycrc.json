{
  "check-coverage": true,
  "extension": [".js"],
  "exclude": [
    "test",
    "lib/**/*.test.js",
    "dangerfile.js"
  ],
  "statements": 80,
  "branches": 75,
  "functions": 80,
  "lines": 80,
  "reporter": ["lcov", "html", "text", "text-summary"],
  "watermarks": {
    "lines": [30, 99],
    "functions": [30, 99],
    "branches": [30, 99],
    "statements": [30, 99]
  }
}
