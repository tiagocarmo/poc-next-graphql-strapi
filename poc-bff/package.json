{
  "name": "project-model-graphql-api",
  "version": "1.0.1",
  "description": "Este projeto tem como finalidade ser um guia para novos projetos utilizando graphql server",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=dev && node index.js",
    "depcheck": "depcheck",
    "coverage": "nyc report",
    "check": "eslint ./**.js .",
    "mocha": "mocha",
    "pretest": "npm run check && npm run depcheck && npm audit --production --audit-level=high",
    "posttest": "nyc check-coverage",
    "start": "export NODE_ENV=production && node index.js",
    "test": "export NODE_ENV=test MINUTRADE_LOGGER_SILENT=1 && nyc --reporter=lcov mocha && npm run coverage",
    "debugtest": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive --inspect-brk"
  },
  "engines": {
    "node": ">=14.17.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minutrade/project-model-graphql-bff.git"
  },
  "keywords": [
    "minutrade",
    "model",
    "example",
    "exemplo",
    "modelo",
    "graphql",
    "bff",
    "apollo-server"
  ],
  "author": "Minutrade",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Minutrade/project-model-graphql-api/issues"
  },
  "homepage": "https://github.com/Minutrade/project-model-graphql-api#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@minutrade/eslint-config-minutrade": "^3.0.1",
    "chai": "^4.2.0",
    "chance": "^1.1.6",
    "depcheck": "^1.2.0",
    "eslint": "^8.14.0",
    "mocha": "^9.2.2",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.10",
    "@graphql-tools/schema": "^8.3.10",
    "@minutrade/bonuz-interactions-client": "^2.0.4",
    "@minutrade/minutrade-logger": "^4.0.1",
    "@minutrade/mtconf": "^1.0.6",
    "apollo-server-express": "^3.6.7",
    "axios": "^0.27.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "danger": "^11.0.5",
    "express": "^4.17.1",
    "graphql": "^16.4.0",
    "helmet": "^5.0.2",
    "ip": "^1.1.5",
    "newrelic": "^8.9.1",
    "ramda": "^0.28.0"
  }
}
